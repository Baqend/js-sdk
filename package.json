{
  "name": "baqend",
  "version": "3.0.0-0",
  "description": "Baqend JavaScript SDK",
  "license": "MIT",
  "author": {
    "name": "Baqend.com",
    "email": "info@baqend.com"
  },
  "homepage": "https://www.baqend.com",
  "main": "./commonjs/lib/index.es5.js",
  "module": "./bundles/baqend.es2015.js",
  "browser": {
    ".": "./bundles/baqend.es5.js",
    "./lib/util/atob.ts": "./lib/util/atob-browser.ts",
    "./lib/util/is-node.ts": "./lib/util/is-node-browser.ts",
    "./lib/util/hmac.ts": "./lib/util/hmac-browser.ts",
    "./lib/util/websocket.ts": "./lib/util/websocket-browser.ts",
    "./lib/connector/NodeConnector": false,
    "./lib/connector/FetchConnector": false
  },
  "react-native": {
    ".": "./commonjs/lib/index.es5.js",
    "./commonjs/lib/util/atob.js": "./commonjs/lib/util/atob-rn.js",
    "./commonjs/lib/util/is-node.js": "./commonjs/lib/util/is-node-browser.js",
    "./commonjs/lib/util/hmac.js": "./commonjs/lib/util/hmac-browser.js",
    "./commonjs/lib/util/websocket.ts": "./commonjs/lib/util/websocket-browser.ts",
    "./commonjs/lib/connector/XMLHttpConnector": false,
    "./commonjs/lib/connector/IFrameConnector": false
  },
  "exports": {
    "browser": {
      "import": "./bundles/baqend.es2015.js",
      "require": "./bundles/baqend.es5.js"
    },
    "default": {
      "import": "./esm/lib/index.es2015.js",
      "require": "./commonjs/lib/index.es5.js"
    }
  },
  "typings": "./esm/lib/index.2015.d.ts",
  "dependencies": {
    "commander": "^2.19.0",
    "glob": "^7.0.6",
    "opn": "^4.0.2",
    "rimraf": "^2.5.4",
    "rxjs": "^6.5.5",
    "simple-git": "^1.107.0",
    "tslib": "^2.0.0",
    "uuid": "^8.1.0",
    "validator": "^13.1.1",
    "websocket": "^1.0.31"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Baqend/js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Baqend/js-sdk/issues"
  },
  "keywords": [
    "backend",
    "baqend",
    "mongodb",
    "sdk",
    "backend-as-a-service"
  ],
  "devDependencies": {
    "@purtuga/esm-webpack-plugin": "^1.2.1",
    "@types/node": "^10.17.26",
    "@types/webpack": "^4.41.17",
    "@types/websocket": "^1.0.0",
    "baqend-coding-standard": "^1.1.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.15.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^8.0.0",
    "events": "^3.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jquery": "^3.1.0",
    "jsdoc": "^3.6.4",
    "karma": "^5.1.0",
    "karma-browserstack-launcher": "^1.6.0",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.9.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coffee-preprocessor": "^1.0.1",
    "karma-firefox-launcher": "^1.3.0",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-testingbot-launcher": "^1.0.3",
    "karma-webdriver-launcher": "^1.0.8",
    "mocha": "^8.0.1",
    "mocha-jenkins-reporter": "^0.4.3",
    "phantomjs-prebuilt": "^2.1.12",
    "puppeteer": "^4.0.1",
    "shelljs": "^0.8.4",
    "shx": "^0.3.2",
    "source-map-support": "^0.5.19",
    "terser-webpack-plugin": "^3.0.6",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "scripts": {
    "test": "npm run test:node && npm run test:karma",
    "test:node": "mocha --config mocharc.json --exit",
    "test:karma": "karma start",
    "clean": "shx rm -rf dist",
    "dev-node": "tsc --watch -m commonjs --target es5 --outDir commonjs",
    "dev-browser": "webpack-dev-server --config webpack.config.js --open",
    "build-commonjs": "tsc -m commonjs --target es5 --outDir commonjs --declaration",
    "build-esm": "tsc -m es2015 --target es6 --outDir esm --declaration",
    "build-bundles": "webpack --config-name=es5 && webpack --config-name=es2015",
    "dist": "npm run build-commonjs && npm run build-esm && npm run build-bundles",
    "release": "node scripts/release",
    "typings": "npm run typings:test",
    "typings:test": "tsc --version && tsc -p spec-ts/tsconfig.json",
    "jsdoc": "jsdoc -t tpl/theme -r lib realtime -c tpl/jsdoc.conf.json --package package.json -d doc",
    "baqend": "node cli/index.js",
    "lint": "eslint lib spec realtime",
    "prepare": ""
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "baqend": "./cli/index.js"
  }
}
