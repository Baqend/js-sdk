{
  "name": "baqend",
  "version": "3.0.0-0",
  "description": "Baqend JavaScript SDK",
  "license": "MIT",
  "author": {
    "name": "Baqend.com",
    "email": "info@baqend.com"
  },
  "homepage": "https://www.baqend.com",
  "main": "./commonjs/lib/index.js",
  "module": "./dist/baqend.es2015.js",
  "browser": {
    ".": "./dist/baqend.es5.js",
    "./lib/util/atob.ts": "./lib/util/atob-browser.ts",
    "./lib/util/is-node.ts": "./lib/util/is-node-browser.ts",
    "./lib/util/hmac.ts": "./lib/util/hmac-browser.ts",
    "./lib/util/websocket.ts": "./lib/util/websocket-browser.ts",
    "./lib/connector/NodeConnector": false,
    "./lib/connector/FetchConnector": false
  },
  "react-native": {
    ".": "./commonjs/lib/index.js",
    "./commonjs/lib/util/atob.js": "./commonjs/lib/util/atob-rn.js",
    "./commonjs/lib/util/is-node.js": "./commonjs/lib/util/is-node-browser.js",
    "./commonjs/lib/util/hmac.js": "./commonjs/lib/util/hmac-browser.js",
    "./commonjs/lib/util/websocket.ts": "./commonjs/lib/util/websocket-browser.ts",
    "./commonjs/lib/connector/XMLHttpConnector": false,
    "./commonjs/lib/connector/IFrameConnector": false
  },
  "exports": {
    "browser": {
      "import": "./dist/baqend.es2015.js",
      "require": "./dist/baqend.es5.js"
    },
    "default": {
      "import": "./esm/lib/index.js",
      "require": "./commonjs/lib/index.js"
    }
  },
  "types": "./esm/lib/index.d.ts",
  "dependencies": {
    "commander": "^2.20.3",
    "glob": "^7.1.6",
    "opn": "^4.0.2",
    "rimraf": "^2.7.1",
    "rxjs": "^6.6.3",
    "simple-git": "^1.132.0",
    "tslib": "^2.0.3",
    "uuid": "^8.3.1",
    "validator": "^13.1.17",
    "websocket": "^1.0.32"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Baqend/js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Baqend/js-sdk/issues"
  },
  "keywords": [
    "backend",
    "baqend",
    "mongodb",
    "sdk",
    "backend-as-a-service"
  ],
  "devDependencies": {
    "@purtuga/esm-webpack-plugin": "^1.5.0",
    "@types/node": "^10.17.47",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "@types/webpack": "^4.41.25",
    "@types/websocket": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "baqend-coding-standard": "^1.1.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "events": "^3.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jquery": "^3.5.1",
    "karma": "^5.2.3",
    "karma-browserstack-launcher": "^1.6.0",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.9.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coffee-preprocessor": "^1.0.1",
    "karma-firefox-launcher": "^1.3.0",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha": "^2.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-testingbot-launcher": "^1.0.3",
    "karma-webdriver-launcher": "^1.0.8",
    "mocha": "^8.2.1",
    "mocha-jenkins-reporter": "^0.4.5",
    "puppeteer": "^4.0.1",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "source-map-support": "^0.5.19",
    "terser-webpack-plugin": "^3.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.7",
    "utils": "^0.3.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "test": "npm run test:node && npm run test:karma",
    "test:build": "",
    "test:node": "mocha --config mocharc.json --exit",
    "test:karma": "karma start",
    "clean": "shx rm -rf dist commonjs esm doc",
    "dev-node-commonjs": "tsc --watch -m commonjs --target es5 --outDir commonjs --declaration --project tsconfig.lib.json",
    "dev-node-esm": "tsc --watch -m es2015 --target es6 --outDir esm --declaration --project tsconfig.lib.json",
    "dev-browser": "webpack-dev-server --config webpack.config.js --open --project tsconfig.lib.json",
    "build-commonjs": "tsc -m commonjs --target es5 --outDir commonjs --project tsconfig.lib.json",
    "build-esm": "tsc -m es2015 --target es6 --outDir esm --declaration --project tsconfig.lib.json",
    "build-dist": "webpack --config-name=es5 && webpack --config-name=es2015  --project tsconfig.lib.json",
    "dist": "npm run build-commonjs && npm run build-esm && npm run build-dist && npm run docs",
    "release": "node scripts/release",
    "typings": "npm run typings:test",
    "typings:test": "tsc --version && tsc -p spec-ts/tsconfig.json",
    "docs": "typedoc --out doc lib",
    "baqend": "node cli/index.js",
    "lint": "eslint --ext .ts lib/",
    "prepare": ""
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "baqend": "./cli/index.js"
  }
}
