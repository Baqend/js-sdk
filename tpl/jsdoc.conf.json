{
  "tags": {
    "allowUnknownTags" : false,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": ""
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles" : true,
      "useLongnameInNav": false
    },
    "applicationName": "Baqend JavaScript SDK Copyright Â© 2015 Baqend GmbH.",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "Baqend JavaScript API Docs",
      "type": "website",
      "image": "",
      "site_name": "Baqend.com",
      "url": "http://www.baqend.com"
    },
    "meta": {
      "title": "Baqend JavaScript API Docs",
      "description": "The Baqend JavaScript API Docs",
      "keyword": "baqend, javascript, sdk, api, jsdoc"
    },
    "linenums": false
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true,
    "tags": ["examples"]
  }
}
